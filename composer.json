{
    "name": "mashbo/mashbot-process-taskrunner-extension",
    "description": "Provide tasks to run commands and processes locally",
    "require": {
        "mashbo/mashbot-taskrunner": "*@dev",
        "symfony/process": "~2.8|~3.0"
    },
    "suggest": {
        "psr/log": "Add extra logging information"
    },
    "autoload": {
        "psr-4": {
            "Mashbo\\Mashbot\\Extensions\\ProcessTaskRunnerExtension\\": "src/",
            "Mashbo\\Mashbot\\Extensions\\ProcessTaskRunnerExtension\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adam Quaile",
            "email": "aquaile@mashbo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Mashbo/mashbot-taskrunner.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.3"
    }
}
